extends _template

block main
	.inner-page.bg-gray
		.inner-page__header
			.container
				ul.breadcrumbs
					li: a(href="#") Главная
					li: a(href="#") Контакты
				h1 Контакты
		.container
			.inner-section
				// home-contacts
				.main-contacts.clearfix
					.col-66
						.contacts-form
							.medium-title Закажите бесплатную консультацию
							.contacts-form__body.clearfix
								form(action="#")
								.col-33
									input(required type="text" placeholder="Ваше имя")
									input(required type="tel" placeholder="Телефон")
									input(required type="email" placeholder="E-mail")
									
								.col-66
									textarea(placeholder="Тест сообщения")
								input(type="submit").btn.btn--brown
					.col-33
						.contacts-block
							.medium-title Посетите наш офис
							.list-name Адрес
							ul
								li м. Щукинская,
								li ул. Маршала Василевского,
								li д. 17, 3 этаж
							.list-name Телефон
							ul
								li.tel +7 (495) 968 30 80
							.list-name Email
							ul
								li uristmsk24@gmail.com
									
				// home-contacts end
				.contacts-map
					<script type="text/javascript" charset="utf-8" async src="https://api-maps.yandex.ru/services/constructor/1.0/js/?sid=1Zt50FNd1Jz1a-u-nyEVhUmrL9g3jrnN&amp;width=100%25&amp;height=350&amp;lang=ru_RU&amp;sourceType=constructor&amp;scroll=true"></script>
